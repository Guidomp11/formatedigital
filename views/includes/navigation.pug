nav.navigation.navbar.navbar-default.navbar-static-top
  div.container
    div.navbar-header
      button.navbar-toggle.collapsed(type="button" data-toggle="collapse" data-target="#navigation" aria-expanded="false")
        span.sr-only Toggle Navigation
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href="/")
        img(src="/img/logo_formatedigital.png" alt=title)
    div.collapse.navbar-collapse#navigation
      unless auth
          include notlogged
      if menu
        //-var selected = 'Home';
        //-unless selected
          -var selected = text('home');
        ul.nav.navbar-nav.navbar-right
          //-each val, key in menu
            if selected === key
              li.active #[a(href=val, title=key)= key]
            else
              li #[a(href=val, title=key)= key]
          if auth
              include logged
